{
    "name": "Login extesion",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "http://*/*",
      "https://*/*"
    ],
    
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    }
  }